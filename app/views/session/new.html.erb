<h1>Session#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<% if flash[:error_message].present? %>
  <p><%= flash[:error_message] %></p>
<% end %>

<!-- form_for is for CRUD, so we won't use that -->


<%= form_tag login_path do %>
  <%= label_tag :email %>
  <%= email_field_tag :email %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= submit_tag "Sign In" %>
<% end %>
